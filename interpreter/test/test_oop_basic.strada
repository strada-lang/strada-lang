package Animal;
has ro str $name (required);
has rw int $energy = 100;

func speak(scalar $self) void {
    say($self->name() . " speaks");
}

package main;
func main() int {
    my scalar $a = Animal::new("name", "Cat", "energy", 50);
    say($a->name());
    say($a->energy());
    $a->set_energy(75);
    say($a->energy());
    $a->speak();
    return 0;
}
